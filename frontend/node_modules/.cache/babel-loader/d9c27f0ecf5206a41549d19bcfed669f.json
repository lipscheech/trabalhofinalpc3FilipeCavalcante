  };\n            http.post(\"/produto\", data).then(response => {\n                this.produto.id = response.data.id;\n                console.log(response.data);\n            })\n            .catch(e => {\n                console.log(e);\n            });\n\n            this.submitted = true;\n        },\n        novoProduto(){\n            this.submitted = false;\n            this.produto = {};\n        }\n    }\n};\n</script>\n\n<style>\n.submitform{\n    max-width: 300px;\n    margin: auto;\n}\n</style>"],"sourceRoot":"src/components"}]}